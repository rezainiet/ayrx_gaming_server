{
    "version": 2,
    "builds": [
        {
            "src": "./index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/socket.io",
            "dest": "/index.js",
            "headers": {
                "cache-control": "s-maxage=0"
            }
        },
        {
            "src": "/socket.io/(.*)",
            "dest": "/index.js",
            "headers": {
                "cache-control": "s-maxage=0"
            }
        },
        {
            "src": "/(.*)",
            "dest": "/index.js"
        }
    ]
}